FoamFile
{
    format      ascii;
    class       dictionary;
    location    "system";
    object      setFieldsDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

defaultFieldValues
(
    volScalarFieldValue alpha.gas 0
    volScalarFieldValue alpha.liquid 1
    volScalarFieldValue alpha.solid 0
);

regions
(
    zoneToCell
    {
        zone            bed;
        fieldValues
        (
            volScalarFieldValue alpha.gas 0
            volScalarFieldValue alpha.liquid 0.4
            volScalarFieldValue alpha.solid  0.6
        );
    }
);


