setDeltaT
{
    libs            ("libutilityFunctionObjects.so");

    type            setTimeStep;

    deltaT
    {
        type            table;
        values
        (
            (-180 0.25)
            (-15 0.025)
        );
        interpolationScheme step;
        outOfBounds     clamp;
    }
}

