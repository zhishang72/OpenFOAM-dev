FoamFile
{
    format      ascii;
    class       dictionary;
    object      limitPressure;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

type       limitPressure;

select     all;

min        <1e4>;

