FoamFile
{
    format      ascii;
    class       LagrangianScalarField;
    location    "0/Lagrangian/kinematicParcel";
    object      number;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [];

internalField   uniform 1e-3;

boundaryField
{
    #includeEtc "caseDicts/setConstraintTypes"

    ".*"
    {
        type            calculated;
    }
}

sources
{
    pointInjection
    {
        type            flowRateNumber;
        uniformSize     volume;
        volumetricFlowRate 0.7 [l/s];
    }
}


