FoamFile
{
    format      ascii;
    class       dictionary;
    location    "system";
    object      functions;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

#includeFunc populationBalanceSizeDistribution
(
    name=numberDensity,
    populationBalance=bubbles,
    functionType=numberDensity,
    coordinateType=diameter,
    normalise=yes
)

#includeFunc populationBalanceSizeDistribution
(
    name=volumeDensity,
    populationBalance=bubbles,
    functionType=volumeDensity,
    coordinateType=diameter,
    normalise=yes
)

