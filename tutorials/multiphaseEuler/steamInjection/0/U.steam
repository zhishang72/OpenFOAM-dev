FoamFile
{
    format      ascii;
    class       volVectorField;
    location    "0";
    object      U.steam;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -1 0 0 0 0];

internalField   uniform (0 0 0);

boundaryField
{
    inlet
    {
        type            fixedValue;
        value           uniform (0 0 0);
    }

    outlet
    {
        type            pressureInletOutletVelocity;
        phi             phi.steam;
        value           $internalField;
    }

    walls
    {
        type            noSlip;
    }
}

sources
{
    massSource
    {
        type            uniformFixedValue;
        uniformValue    (0 10 0);
    }
}

