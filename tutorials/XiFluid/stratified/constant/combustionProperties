laminarFlameSpeed
{
    model           unstrained;

    unstrainedLaminarFlameSpeed
    {
        model           Gulder;

        equivalenceRatio 1;

        fuel            Propane;

        Propane
        {
            W               0.446;
            eta             0.12;
            xi              4.95;
            alpha           1.77;
            beta            -0.2;
            f               2.3;
        }
    }
}

flameWrinkling
{
    model           transport;

    equilibrium
    {
        model           Gulder;
    }

    profile
    {
        model      linear;
    }

    generationRate
    {
        model           KTS;
    }
}


