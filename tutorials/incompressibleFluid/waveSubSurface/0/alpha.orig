FoamFile
{
    format      ascii;
    class       volScalarField;
    location    "0";
    object      alpha;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [];

internalField   uniform 1;

boundaryField
{
    bottom
    {
        type            zeroGradient;
    }

    "(top|inlet|outlet)"
    {
        type            waveAlpha;
        libs            ("libwaves.so");
    }

    "(front|back)"
    {
        type            symmetryPlane;
    }
}


