FoamFile
{
    format      ascii;
    class       dictionary;
    location    "system";
    object      functions;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

#includeFunc patchFlowRate(name=inletFlowRate, patch=inlet)
#includeFunc patchFlowRate(name=outletFlowRate, patch=outlet)

cartesianToCylindrical
{
    type        cylindrical;
    libs        ("libfieldFunctionObjects.so");

    origin      (0 0 0);
    axis        (1 0 0);

    field       U;

    writeControl    writeTime;
    writeInterval   1;
}

