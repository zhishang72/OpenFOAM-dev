FoamFile
{
    format      ascii;
    class       dictionary;
    location    "system";
    object      createBafflesDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

internalFacesOnly true;

baffles
{
    wall
    {
        type        faceZone;
        zoneName    baffles;

        owner
        {
            name        baffles;
            type        wall;
        }
        neighbour
        {
            $owner;
        }
    }
}


