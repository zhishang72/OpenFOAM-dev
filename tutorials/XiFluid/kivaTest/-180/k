FoamFile
{
    format      ascii;
    class       volScalarField;
    location    "-180";
    object      k;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -2 0 0 0 0];

internalField   uniform 4;

boundaryField
{
    piston
    {
        type            kqRWallFunction;
        value           $internalField;
    }
    liner
    {
        type            kqRWallFunction;
        value           $internalField;
    }
    cylinderHead
    {
        type            kqRWallFunction;
        value           $internalField;
    }
}


