FoamFile
{
    format      ascii;
    class       dictionary;
    location    "system";
    object      fvConstraints;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

limitp
{
    type       limitPressure;

    minFactor  0.4;
    maxFactor  1.5;
}

fixedTemperature
{
    type            fixedTemperature;

    cellZone        porosity;

    mode            uniform;
    temperature     350;
}

porosityTurbulence
{
    type            fixedValue;

    cellZone        porosity;

    fieldValues
    {
        k           1;
        epsilon     150;
    }
}


