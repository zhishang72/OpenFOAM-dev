FoamFile
{
    format      ascii;
    class       dictionary;
    location    "constant";
    object      fvModels;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

airToPorous
{
    type            interRegionHeatTransfer;

    libs            ("libinterRegionFvModels.so");

    nbrRegion       porous;
    master          false;

    interpolationMethod intersection;

    semiImplicit    no;
}

porosity
{
    type            interRegionPorosityForce;

    libs            ("libinterRegionFvModels.so");

    interRegionPorosityForceCoeffs
    {
        nbrRegion       porous;

        interpolationMethod intersection;

        type            DarcyForchheimer;

        d   (-1000 -1000 1e4);
        f   (0 0 0);

        coordinateSystem
        {
            type    cartesian;
            origin  (0 0 0);
            coordinateRotation
            {
                type    axesRotation;
                e1      (0 1 0);
                e2      (0 0 1);
            }
        }
    }
}


