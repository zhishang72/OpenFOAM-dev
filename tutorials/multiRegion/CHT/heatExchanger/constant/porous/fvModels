// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

porousToAir
{
    type            interRegionHeatTransfer;

    libs            ("libinterRegionFvModels.so");

    nbrRegion       air;
    master          true;

    interpolationMethod intersection;

    semiImplicit    no;

    Av              200;

    heatTransferCoefficientModel constant;

    constantCoeffs
    {
        htc             10;
    }
}


