FoamFile
{
    format      ascii;
    class       dictionary;
    location    "constant";
    object      fvModels;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

phaseTurbulenceStabilisationGas
{
    type            phaseTurbulenceStabilisation;
    libs            ("libmultiphaseEulerFvModels.so");
    phase           gas;
    alphaInversion  0.1;
}

phaseTurbulenceStabilisationLiquid
{
    type            phaseTurbulenceStabilisation;
    libs            ("libmultiphaseEulerFvModels.so");
    phase           liquid;
    alphaInversion  0.1;
}

bedHeat
{
    type            semiImplicitSource;

    selectionMode   cellZone;
    cellZone        bed;

    volumeMode      absolute;

    sources
    {
        e.solid
        {
            explicit
            {
                type            scale;
                value           2.3e4;
                scale
                {
                    type            linearRamp;
                    start           0.05;
                    duration        0.1;
                }
            }

            implicit        0;
        }
    }
}


