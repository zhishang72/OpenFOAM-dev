FoamFile
{
    format      ascii;
    class       volScalarField;
    location    "0";
    object      p_rgh;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -1 -2 0 0 0 0];

internalField   uniform 0;

boundaryField
{
    #includeEtc "caseDicts/setConstraintTypes"

    wall
    {
        type            fixedFluxPressure;
        value           $internalField;
    }
    bottom
    {
        type            prghCyclicPressure;
        patchType       cyclic;
        rhoInf          1;
    }
    top
    {
        type            prghCyclicPressure;
        patchType       cyclic;
    }
}


