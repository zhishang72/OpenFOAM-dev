FoamFile
{
    format      ascii;
    class       dictionary;
    object      physicalProperties;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

thermoType
{
    type            heRhoThermo;
    mixture         pureMixture;
    transport       const;
    thermo          eConst;
    equationOfState rhoConst;
    specie          specie;
    energy          sensibleInternalEnergy;
}

mixture
{
    specie
    {
        nMoles      1;
        molWeight   18;         // [kg/kmol]
    }
    equationOfState
    {
        rho         998.19;     // [kg/m^3]
    }
    thermodynamics
    {
        Cv          4150;       // [J/kg/K]
        hf          0;
    }
    transport
    {
        mu          1.0005e-3;  // [kg/m/s]
        Pr          6.31;
    }
}


